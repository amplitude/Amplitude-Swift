#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/replace-in-file@6.3.5/node_modules/replace-in-file/bin/node_modules:/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/replace-in-file@6.3.5/node_modules/replace-in-file/node_modules:/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/replace-in-file@6.3.5/node_modules:/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/replace-in-file@6.3.5/node_modules/replace-in-file/bin/node_modules:/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/replace-in-file@6.3.5/node_modules/replace-in-file/node_modules:/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/replace-in-file@6.3.5/node_modules:/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../replace-in-file@6.3.5/node_modules/replace-in-file/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../replace-in-file@6.3.5/node_modules/replace-in-file/bin/cli.js" "$@"
fi
