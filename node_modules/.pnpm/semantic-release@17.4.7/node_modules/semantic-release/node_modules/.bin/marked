#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/marked@2.1.3/node_modules/marked/bin/node_modules:/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/marked@2.1.3/node_modules/marked/node_modules:/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/marked@2.1.3/node_modules:/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/marked@2.1.3/node_modules/marked/bin/node_modules:/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/marked@2.1.3/node_modules/marked/node_modules:/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/marked@2.1.3/node_modules:/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../marked@2.1.3/node_modules/marked/bin/marked" "$@"
else
  exec node  "$basedir/../../../../../marked@2.1.3/node_modules/marked/bin/marked" "$@"
fi
