#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/conventional-changelog-writer@4.1.0/node_modules/conventional-changelog-writer/node_modules:/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/conventional-changelog-writer@4.1.0/node_modules:/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/conventional-changelog-writer@4.1.0/node_modules/conventional-changelog-writer/node_modules:/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/conventional-changelog-writer@4.1.0/node_modules:/Users/danielgraham/code/Amplitude-Swift/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../conventional-changelog-writer/cli.js" "$@"
else
  exec node  "$basedir/../conventional-changelog-writer/cli.js" "$@"
fi
